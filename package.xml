	xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
	<name>ckWebServicePlugin</name>
	<channel>plugins.symfony-project.org</channel>
	<summary>WebService API Plugin.</summary>
	<description>
		The ckWebServicePlugin allows you to build a webservice api for your symfony applications. It comes with a powerful and easy to use wsdl generator, which creates WS-I compliant wsdl files for a maximum of interopability with PHP, .NET and Java clients.
	</description>
	<lead>
		<name>Christian Kerl</name>
		<user>chrisk</user>
		<email>christian-kerl@web.de</email>
		<active>yes</active>
	</lead>
	<date>2009-08-08</date>
	<version>
		<release>2.3.0</release>
		<api>2.3.0</api>
	</version>
	<stability>
		<release>stable</release>
		<api>stable</api>
	</stability>
	<license uri="http://www.opensource.org/licenses/mit-license.php">
		MIT license
	</license>
	<notes>-</notes>
